<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/nav_graph"
app:startDestination="@id/nav_home">

<fragment
    android:id="@+id/nav_home"
    android:name="com.example.techtonic.Fragments.Home"
    android:label="Home"
    tools:layout="@layout/fragment_home" >
    <action
        android:id="@+id/action_nav_home_to_nav_notifications"
        app:destination="@id/nav_notifications" />
    <action
        android:id="@+id/action_nav_home_to_nav_settings"
        app:destination="@id/nav_settings" />
    <action
        android:id="@+id/action_nav_home_to_nav_profile"
        app:destination="@id/nav_profile" />
    <action
        android:id="@+id/action_nav_home_to_nav_reports"
        app:destination="@id/nav_reports" />
    <action
        android:id="@+id/action_nav_home_to_nav_profile2"
        app:destination="@id/nav_profile" />
</fragment>

<fragment
    android:id="@+id/nav_reports"
    android:name="com.example.techtonic.Fragments.Report"
    android:label="Reports"
    tools:layout="@layout/fragment_report" />

<fragment
    android:id="@+id/nav_notifications"
    android:name="com.example.techtonic.Fragments.Notification"
    android:label="Notifications"
    tools:layout="@layout/fragment_notification" />

<fragment
    android:id="@+id/nav_profile"
    android:name="com.example.techtonic.Fragments.Profile"
    android:label="Profile"
    tools:layout="@layout/fragment_profile" />

<fragment
    android:id="@+id/nav_settings"
    android:name="com.example.techtonic.Fragments.Settings"
    android:label="Settings"
    tools:layout="@layout/fragment_settings" />
</navigation>
